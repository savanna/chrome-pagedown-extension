{
    "name": "Pagedown Extensions",
    "description": "Provides a pagedown onscreen button on every page.",
    "version": "1.0.16",
    "manifest_version": 2,

    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "css": ["content.css"],
        "js": ["content.js"]
    }]
}
